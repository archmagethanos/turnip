using ProgressMeter
using PolynomialRoots
using CSV

function readFileLines(fileName::String)
    file = open(fileName,"r")

    seekend(file)
    fileSize = position(file)

    seekstart(file)
    p = Progress(fileSize, 1)   # minimum update interval: 1 second
    while !eof(file)
        line = readline(file)
        

        update!(p, position(file))
    end
end



@assert str[1] == '[' && str[end] == ']'

CSV.read(IOBuffer(str[2:prevind(str, end)]), header=false, transpose=true)

filename = "data/q_roots_30.csv"

CSV.chunks(filename; transpose = true, )

